@page
@inherits TCX.Student.Web.Pages.StudentPageBaseModel
@model TCX.Student.Web.Pages.Class.CreateModalModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@{
    Layout = null;
}

<abp-dynamic-form abp-model="Class" data-ajaxForm="true" asp-page="/Class/CreateModal">
    <abp-modal>
        <abp-modal-header title="Thêm Class"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>

